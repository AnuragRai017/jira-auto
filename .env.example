# Environment Configuration for Jira Automation
# Copy this file to .env and update with your actual values

# Example environment variables - Copy to .env and fill with your values
# Do not commit .env file to version control

# Jira Configuration
JIRA_URL=https://your-domain.atlassian.net
JIRA_EMAIL=your-email@domain.com
JIRA_API_TOKEN=your-api-token-here

# Runtime Configuration
NODE_ENV=development
DEBUG=true

# Project Configuration  
PROJECT_KEY=TS
CUSTOMER_FIELD_ID=customfield_10485

# Optional: Default project key
DEFAULT_PROJECT_KEY=TS

# Optional: Automation settings
MAX_BULK_TRANSFER_SIZE=100
LOG_LEVEL=INFO
DRY_RUN_DEFAULT=false

# Team Lead Email Mappings (update with actual emails)
OPS_TECHNICAL_LEAD=john.doe@company.com
OPS_INFRASTRUCTURE_LEAD=jane.smith@company.com
OPS_USER_SUPPORT_LEAD=mike.johnson@company.com
OPS_ESCALATION_LEAD=sarah.wilson@company.com
OPS_SECURITY_LEAD=security.lead@company.com
OPS_DATABASE_LEAD=database.admin@company.com
OPS_CHANGE_MANAGEMENT_LEAD=change.manager@company.com
OPS_GENERAL_LEAD=ops-manager@company.com

# Optional: Webhook URLs for notifications
SLACK_WEBHOOK_URL=https://hooks.slack.com/your-webhook-url
TEAMS_WEBHOOK_URL=https://your-company.webhook.office.com/your-webhook-url

